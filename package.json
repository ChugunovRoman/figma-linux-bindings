{
  "name": "bindings",
  "version": "0.1.0",
  "description": "C++ module that makes object of meta data of fonts",
  "scripts": {
    "start": "node --experimental-modules index.mjs",
    "build": "node-gyp configure && node-gyp build --static -j `nproc`",
    "build-dev": "node-gyp configure && node-gyp build --static --debug -j `nproc`",
    "rebuild": "HOME=~/.electron-gyp node-gyp rebuild  --static --target=4.1.5 --arch=x64 --dist-url=https://atom.io/download/electron -j `nproc`"
  },
  "keywords": [
    "C++",
    "nodejs",
    "ttf",
    "fonts",
    "Linux"
  ],
  "author": "Chugunov Roman",
  "license": "gpl-2.0",
  "devDependencies": {
    "node-gyp": "^3.8.0"
  },
  "dependencies": {
    "nan": "^2.11.1"
  }
}
